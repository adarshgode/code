class Solution {
    Set<String> exactMatch = new HashSet<>();
    Map<String,String> caseMatch = new HashMap<>();
    Map<String,String> owelMatch = new HashMap<>();
    public String[] spellchecker(String[] wordlist, String[] queries) {
        for(String str : wordlist){
            exactMatch.add(str);
            String lowerCase = str.toLowerCase();
            if(!caseMatch.containsKey(lowerCase)){
                caseMatch.put(lowerCase,str);
            }
            String owelMismatch = owelMismatch(lowerCase);
            if(!owelMatch.containsKey(owelMismatch)){
                owelMatch.put(owelMismatch,str);
            }
        }

        String[] result = new String[queries.length];

        for(int i=0;i<queries.length;i++){
            String str = queries[i];
            String lowerCase = str.toLowerCase();
            String owelMismatch = owelMismatch(lowerCase);
            if(exactMatch.contains(str)){
                result[i] = str;
            }else if(caseMatch.containsKey(lowerCase)){
                result[i] = caseMatch.get(lowerCase);
            }else if(owelMatch.containsKey(owelMismatch)){
                result[i] = owelMatch.get(owelMismatch);
            }else{
                result[i] = "";
            }
        }
        return result;
    }

    public String owelMismatch(String str){
        StringBuilder sb = new StringBuilder();
        for(int i=0;i<str.length() ; i++){
            char c = str.charAt(i);
            if(c=='a' || c == 'i' || c== 'e' || c == 'o' || c == 'u'){
                sb.append('*');
            }else{
                sb.append(c);
            }
        }
        return sb.toString();
    }
}
