class Solution {
    public int[] nextGreaterElements(int[] nums) {
        int n = nums.length;
        int[] nge  = new int[n];
        for(int i=0;i<n;i++){
            int temp = -1;
            for(int j=i+1;j<i+n;j++){
                if(nums[i]<nums[j%n]){
                    temp = nums[j%n];
                    break;
                }
            }
            nge[i] = temp;
        }
        return nge;
    }
}
